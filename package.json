{
  "name": "winston-firetruck",
  "version": "0.0.1",
  "description": "Tiny, dead simple Kinesis Firehose transport for Winston.js",
  "main": "dist/index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "npm run lint && ./node_modules/.bin/babel index.js -d dist/",
    "lint": "eslint .",
    "publish": "npm run build && npm run publish",
    "release": "./node_modules/.bin/release-it",
    "release:patch": "npm run release patch",
    "release:minor": "npm run release minor",
    "release:major": "npm run release major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elifitch/winston-firetruck.git"
  },
  "keywords": [
    "winston",
    "winston",
    "transport",
    "logging",
    "kinesis",
    "firehose",
    "kinesis",
    "firehose",
    "winston",
    "aws"
  ],
  "author": "Eli Fitch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elifitch/winston-firetruck/issues"
  },
  "homepage": "https://github.com/elifitch/winston-firetruck#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.31",
    "@babel/core": "^7.0.0-beta.31",
    "@babel/polyfill": "^7.0.0-beta.31",
    "@babel/preset-env": "^7.0.0-beta.31",
    "aws-sdk": "^2.145.0",
    "eslint": "^4.10.0",
    "release-it": "^3.2.0"
  },
  "peerDependencies": {
    "winston": "^3.0.0"
  }
}
